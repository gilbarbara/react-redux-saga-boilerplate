  "scripts": {
    "start": "APP_ENV=development node tools/start.js",
    "build": "NODE_ENV=production APP_ENV=production webpack --config config/webpack.config.prod.js",
    "build:pages": "NODE_ENV=production APP_ENV=pages webpack --config config/webpack.config.prod.js",
    "analyze": "NODE_ENV=production webpack --config config/webpack.prod.js --profile --json > webpack.stats.json",
    "lint": "eslint  --ext .js --ext .jsx app/scripts config test",
    "lint:styles": "stylelint app/styles/**/*.scss",
    "test": "jest --config config/jest.config.js --coverage",
    "test:watch": "jest --config config/jest.config.js --watch",
    "test:ui": "nightwatch -c test/__setup__/nightwatch.conf.js",
    "test:ui:start": "npm start -- test:ui",
    "bundlesize": "bundlesize",
    "deploy": "node tools deploy",
    "publish": "node tools publish",
    "docs": "node tools docs",
    "precommit": "node tools commits && npm run lint && npm run lint:styles && npm test && npm run bundlesize",
    "postmerge": "node tools update && npm update"
  },
