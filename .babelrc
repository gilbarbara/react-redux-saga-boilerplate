{
  "presets": [
    [
      "env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "modules": false
    }
    ],
    "react",
    "stage-1"
  ],
  "plugins": [
    [
      "transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }
    ]
  ],
  "compact": "true",
  "env": {
    "production": {
      "plugins": [
        "lodash",
        "array-includes",
        "transform-flow-strip-types"
      ]
    },
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ],
      "sourceMaps": "both"
    }
  }
}
